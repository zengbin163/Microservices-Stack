<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chihuo.food.domain.category.repository.mapper.CategoryTypeDao">

    <select id="queryCategoryTypeList" resultType="com.chihuo.food.domain.category.repository.po.CategoryTypePO">
        select id id,
        	   type_name typeName,
        	   create_time createTime,
        	   update_time updateTime
        from category_type
        order by id asc
    </select>

</mapper>
